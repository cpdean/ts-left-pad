{
  "name": "ts-left-pad",
  "version": "0.0.2",
  "description": "the only type safe way to pad a string on the left-handed side of the string",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run build_tests && jasmine",
    "tsc": "tsc",
    "typings": "typings",
