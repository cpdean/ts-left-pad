  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cpdean/ts-left-pad/issues"
  },
  "homepage": "https://github.com/cpdean/ts-left-pad#readme",
  "devDependencies": {
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  }
}
